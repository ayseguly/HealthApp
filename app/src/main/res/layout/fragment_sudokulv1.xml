                android:layout_width="58dp"
                android:layout_height="match_parent"
                android:ems="10"

                android:inputType="numberDecimal"
                android:textAlignment="center"
                android:textColor="#020202" />

            <EditText
                android:id="@+id/sudoku1lv12"
                android:layout_width="58dp"
                android:layout_height="match_parent"
